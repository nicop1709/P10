{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "modelBlob",
      "type": "blob",
      "direction": "in",
      "path": "models/als_model.pkl",
      "connection": "AzureWebJobsStorage",
      "dataType": "binary"
    },
    {
      "name": "metadataBlob",
      "type": "blob",
      "direction": "in",
      "path": "models/metadata.pkl",
      "connection": "AzureWebJobsStorage",
      "dataType": "binary"
    },
    {
      "name": "csrBlob",
      "type": "blob",
      "direction": "in",
      "path": "models/csr_train.pkl",
      "connection": "AzureWebJobsStorage",
      "dataType": "binary"
    }
  ]
}

